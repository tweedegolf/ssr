{
  "name": "react-server-silex",
  "version": "0.0.1",
  "description": "React server side rendering with Silex",
  "main": "index.js",
  "scripts": {
    "bn": "babel-node",
    "silex": "php -S localhost:8080",
    "express": "babel-node ./src/js/server.js",
    "silex_twig": "TWIG=1 php -S localhost:8080",
    "express_twig": "TWIG=1 babel-node ./src/js/server.js",
    "watch": "watchify ./src/js/browser.js -t [ babelify ] -o ./assets/bundle.js --debug --verbose"
  },
  "repository": {
    "type": "git",
    "url": "https://tgrep.nl/prototype/ssr/"
  },
  "keywords": [
    "react",
    "react server",
    "reactjs",
    "router5",
    "silex"
  ],
  "devDependencies": {
    "babel-cli": "6.23.0",
    "babel-core": "^6.10.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "6.22.0",
    "babelify": "7.3.0",
    "eslint": "3.16.1",
    "eslint-config-airbnb": "14.1.0",
    "eslint-plugin-babel": "4.0.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.0",
    "node-sass": "^3.8.0",
    "watchify": "3.9.0"
  },
  "dependencies": {
    "babel-polyfill": "6.23.0",
    "classnames": "2.2.5",
    "express": "4.14.1",
    "flux": "3.1.2",
    "ramda": "0.23.0",
    "react": "^15.2.0",
    "react-dom": "^15.2.0",
    "react-router5": "4.0.1",
    "router5": "4.5.1"
  }
}
